                    5534   5586 ;
                    5535   5587 ; CASSETTE FILE FORMAT
                    5536   5588 ;
                    5537   5589 ;    EACH RECORD:
                    5538   5590 ;       TYPE BYTE: 4 FOR BASIC PROGRAM,
                    5539   5591 ;                  PLUS BIT 7 ON IF DATA NOT HEADER RECORD
                    5540   5592 ;       LENGTH BYTE: # DATA BYTES (1-128)

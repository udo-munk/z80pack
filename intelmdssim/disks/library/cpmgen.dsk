                ;
                ;	CHECK DISK STATUS
 3022 DB79      	IN	RTYPE
 3024 E603      	ANI	11B
 3026 FE02      	CPI	2
                ;
                	IF	TESTING
                	CNC	RMON80	;GO TO MONITOR IF 11 OR 10
                	ENDIF
                	IF	NOT TESTING
 3028 D20030    	JNC	RSTART	;RETRY THE LOAD
                	ENDIF
                ;
